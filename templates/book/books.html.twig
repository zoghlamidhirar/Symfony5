<h1>List Books</h1>

{% if tabBooks is empty %}
<h1>Tab vide</h1>
{% else %}

<table border="1">
  <tr>
    <td>Ref</td>
    <td>Title</td>
    <td>Category</td>
    <td>publicationDate</td>
  </tr>
  {% for book in tabBooks %}
  <tr>
    <td>{{ book.ref }}</td>
    <td>{{ book.title  }}</td>
    <td>{{ book.category }}</td>
    <td>{{ book.publicationDate | date }}</td>
    
    </td>
  </tr>

  {% endfor %}
</table>

{% endif %}
